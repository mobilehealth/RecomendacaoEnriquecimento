	public void run() {
		List<Person> people = new ArrayList<Person>();
		System.out.println("\nConstrução de Persons:");

		for (int i = 0; i < size; i++) {

			Person person = new Person();
			person.setGender(generateGender(i));
			person.setDateBirth(generateDateBirth(i));
			person.setIncome(generateIncome(i));			
			person.setDisease(generateDisease(i));
			
			person.setReligion(i);
			person.setRace(i);
			
			person.setLearning(generateLearning(i));
						
			//PESOS DA RECOMENDAÇÃO
			person.setP1(5);
			person.setP2(5);
			person.setP3(5);	

			//SEMELHANÇA SEMÂNTICA
			person.setSS1(generateSemelhanca(i, 1));
			person.setSS2(generateSemelhanca(i, 2));
		  //person.setSS3(generateSemelhanca(i, 3));
		  //person.setSS4(generateSemelhanca(i, 4));
			
			generateName(person, people, person.getGender());
			String passEncripted = SecurityUbi.encodeMD5(person.getNameFirst());

			person.setPassword(passEncripted);
			
			Context context = generateContexto(person);
			
			person.getListContexts().add(context);

			people.add(person);

		}
		
		System.out.println("\nGerados:" + people.size()+" Persons.");
		saveServer(people);
		System.out.println("\nSalvos:" + people.size()+" Persons.");

		//saveInFile(people);
	}

	private void saveServer(List<Person> people) {
		for(Person person : people) {
			managerServer.createPerson(person);
		}
		
	}

	private void saveInFile(List<Person> people) {

		try {
			PrintWriter out = new PrintWriter("teste.txt");

			for (Person person : people) {
				// Apagar esse trecho depois
				String strdate = null;

				SimpleDateFormat sdf = new SimpleDateFormat("MM/dd/yyyy");

				if (person.getDateBirth() != null) {
					strdate = sdf.format(person.getDateBirth().getTime());
				}

				String imprimir = person.getNameFirst() + "|"
						+ person.getNameLast() + "|" + person.getEmail() + "|"
						+ person.getGender() + "|" + strdate + "|"
						+ person.getIncome() + "|" + person.getPassword();

				out.println(imprimir);
			}
			out.close();
		} catch (FileNotFoundException e) {
			e.printStackTrace();
		}

	}

	private void generateName(Person person, List<Person> people, int gender) {

		String firstName = "";
		String lastName = "";
		String email = "";

		boolean validName = false;

		while (!validName) {
			validName = true;

			firstName = randomName("", gender);
			lastName = randomName(firstName, -1);

			for (Person p : people) {
				if (firstName.equals(p.getNameFirst())
						&& lastName.equals(p.getNameLast())) {
					validName = false;
					break;
				}
			}
		}

		email = firstName + "." + lastName + "@email.com.br";

		person.setNameFirst(firstName);
		person.setNameLast(lastName);
		person.setEmail(email);

	}

	private String randomName(String excludeName, int gender) {

		boolean valid = false;
		String name = "";
		Random random = new Random();
		List<String> names;

		// Sobrenome
		if (gender == -1) {
			names = getSurname();
			// Masculino
		} else if (gender == MALE) {
			names = getManNames();
		} else {
			names = getWomanNames();
		}

		while (!valid) {
			int index = random.nextInt(names.size() - 1);

			if (!excludeName.equals(names.get(index))) {
				name = names.get(index);
				valid = true;
			}

		}

		return name;
	}

	private float generateSemelhanca(int i, int k) {

		Random random = new Random();
		float SS = 0;
		float range = random.nextFloat()*3;

		if (((int) (i + tamanhoDaFaixa)/tamanhoDaFaixa) == k ) {
			SS = range + 7;
		} else {
			SS = range;
		}
		if (i >= tamanhoDaFaixa*2)
	//	if (i >= tamanhoDaFaixa*4) 
			SS = range;
		
		return SS;
	}

	private Context generateContexto(Person person) {
		Context context = new Context();
		context.setPerson(person);
		context.setLocation(null);
		context.setBattery(50);
		context.setConnection(Context.CONNECTION_WIFI);
		context.setConnectionSpeed(300);
		context.setDevice(null);
		context.setPersonSpeed(0);
		context.setType(Context.TYPE_PERSON_CREATED_FIRST);
		context.setTimeStamp(null);
		
		return context;
	}

