	private int generateGender(int i) {

		Random random = new Random();
		float acaso = random.nextFloat();
		// Padrão Feminino
		int gender = FEMALE;

		// Masculino
		if (acaso < 0.4852) { // 47,9%
			gender = MALE;
		}

		return gender;
	}

	private Calendar generateDateBirth(int i) {

		int year, month, day;
		int age = 0;

		Calendar dateBirth = Calendar.getInstance();

		Calendar currentDate = Calendar.getInstance();
		int currentYear = currentDate.get(Calendar.YEAR);
		int currentMonth = currentDate.get(Calendar.MONTH);
		int currentDay = currentDate.get(Calendar.DAY_OF_MONTH);

		Random random = new Random();
		float acaso = random.nextFloat();
		// 10 - 19
		if (acaso < 0.205) { // 20,50%
			age = random.nextInt(10) + 10;
			// 20-29
			} else if (acaso < 0.3985) { // 19,35%
				age = random.nextInt(10) + 20;
				// 30-49
			} else if (acaso < 0.7357) { // 33,73%
				age = random.nextInt(20) + 30;
				// 50 - 69
			} else if (acaso < 0.9035) { // 16,77%
				age = random.nextInt(20) + 50;
				// acima de 69
			} else { // 9,65%
				age = random.nextInt(30) + 70;
			}
/*		
		// 10 - 19
		if (acaso < 0.205) { // 20,50%
			age = random.nextInt(10) + 10;
			// 20-29
		} else if (acaso < 0.3985) { // 19,35%
			age = random.nextInt(10) + 20;
			// 30-49
		} else if (acaso < 0.7357) { // 33,73%
			age = random.nextInt(20) + 30;
			// 50 - 69
		} else if (acaso < 0.9035) { // 16,77%
			age = random.nextInt(20) + 50;
			// acima de 69
		} else { // 9,65%
			age = random.nextInt(30) + 70;
		}
*/
		year = currentYear - age;
		month = random.nextInt(currentMonth + 1);
		if (month == currentMonth) {
			day = random.nextInt(currentDay) + 1;
		} else {
			dateBirth.set(year, month, 1);
			dateBirth.getActualMaximum(Calendar.DAY_OF_MONTH);
			day = dateBirth.get(Calendar.DAY_OF_MONTH);
		}

		dateBirth.set(year, month, day);

		return dateBirth;
	}

	private float generateIncome(int i) {

		float minimumWage = 724;
		float income = 0;
		int range = 0;

		Random random = new Random();
		float acaso = random.nextFloat();
		
		// Até 1/4 salário, sem rendimento ou não declarou
		if (acaso < 0.138) { // 13,8% 
			range = (int) (minimumWage / 4);
			income = random.nextInt(range);
			// 1/4 a 1/2 salário
		} else if (acaso < 0.279) { // 14,1%
			range = (int) (minimumWage / 4);
			income = random.nextInt(range) + range;
			// de 1/2 a 1 salario
		} else if (acaso < 0.549) { // 27,0%
			range = (int) (minimumWage / 2);
			income = random.nextInt(range) + range;
			// de 1 a 2 salarios
		} else if (acaso < 0.807) { // 25,8% 
			range = (int) minimumWage;
			income = random.nextInt(range) + minimumWage;
			// de 2 a 5 salarios 
		} else if (acaso < 0.950){ // 14,3%
			range = (int) minimumWage * 3;
			income = random.nextInt(range) + (minimumWage * 2);
			// acima de 5 salarios
		} else { // 5,0%
			range = (int) minimumWage * 10;
			income = random.nextInt(range) + (minimumWage * 5);
		}

		return income;
	}

