<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mobilehealth.umc"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="18"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".view.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.GlucoseMonitor"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="mobilehealth.umc.view.TemperatureMonitor"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="mobilehealth.umc.view.UserRegistration"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="mobilehealth.umc.view.WeightMonitor"
            android:label="@string/app_name" >
        </activity>
    </application>

</manifest>
